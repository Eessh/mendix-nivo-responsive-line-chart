                    <enumerationValue key="log">Log</enumerationValue>
                    <enumerationValue key="symlog">Sym Log</enumerationValue>
                    <enumerationValue key="point">Point</enumerationValue>
                    <enumerationValue key="band">Band</enumerationValue>
